<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Sequences for op number -->
        <record id="seq_op" model="ir.sequence">
            <field name="name">OP</field>
            <field name="code">op.card</field>
            <field name="prefix">OP/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="token_no" model="ir.sequence">
            <field name="name">Token No</field>
            <field name="code">increment_token_no</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
